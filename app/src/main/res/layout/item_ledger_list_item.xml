<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="wrap_content"
    android:layout_marginBottom="2dp"
    xmlns:card_view="http://schemas.android.com/apk/res-auto">

<!--    <android.support.v7.widget.CardView-->

<!--     -->
<!--        android:layout_width="match_parent"-->
<!--        android:layout_height="wrap_content"-->
<!--        card_view:cardBackgroundColor="#F8F8F8"-->
<!--        android:layout_marginTop="5dp">-->

        <LinearLayout

            android:id="@+id/relnear"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:layout_marginTop="5dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <ImageView
                android:visibility="invisible"
                android:id="@+id/Image3Dot"
                card_view:srcCompat="@drawable/ic_more_vert_black_24dp"
                android:background="@android:color/transparent"
                android:textAlignment="center"
                android:textSize="@dimen/textSize"
                android:layout_width="16dp"
                android:layout_height="wrap_content" />
            <TextView
                android:id="@+id/textview1"
                android:textAlignment="center"
                android:textSize="@dimen/textSize"
                android:layout_margin="3dp"
                android:layout_width="@dimen/hundred_dp"
                android:layout_height="wrap_content"
                android:text="Date"/>

            <TextView
                android:id="@+id/textview2"
                android:textAlignment="center"
                android:textSize="@dimen/textSize"
                android:layout_margin="3dp"
                android:layout_width="@dimen/hundred_dp"
                android:layout_height="wrap_content"
                android:text="BillNo"/>

            <TextView
                android:id="@+id/textview3"
                android:textAlignment="center"
                android:textSize="@dimen/textSize"
                android:layout_margin="3dp"
                android:layout_width="@dimen/hundred_dp"
                android:layout_height="wrap_content"
                android:text="EntryType"/>

            <TextView
                android:id="@+id/textview4"
                android:textAlignment="center"
                android:textSize="@dimen/textSize"
                android:layout_margin="3dp"
                android:layout_width="@dimen/hundred_dp"
                android:layout_height="wrap_content"
                android:text="Account"/>

            <TextView
                android:id="@+id/textview5"
                android:textAlignment="center"
                android:textSize="@dimen/textSize"
                android:layout_margin="3dp"
                android:layout_width="@dimen/hundred_dp"
                android:layout_height="wrap_content"
                android:text="Particular"/>

            <TextView
                android:id="@+id/textview6"
                android:textAlignment="center"
                android:textSize="@dimen/textSize"
                android:layout_margin="3dp"
                android:layout_width="@dimen/hundred_dp"
                android:layout_height="wrap_content"
                android:text="Price"/>

            <TextView
                android:id="@+id/textview7"
                android:textAlignment="center"
                android:textSize="@dimen/textSize"
                android:layout_margin="3dp"
                android:layout_width="@dimen/hundred_dp"
                android:layout_height="wrap_content"
                android:text="Add"/>
            <TextView
                android:id="@+id/textview8"
                android:textAlignment="center"
                android:textSize="@dimen/textSize"
                android:layout_margin="3dp"
                android:layout_width="@dimen/hundred_dp"
                android:layout_height="wrap_content"
                android:text="Less"/>
            <TextView
                android:id="@+id/textview9"
                android:textAlignment="center"
                android:textSize="@dimen/textSize"
                android:layout_margin="3dp"
                android:layout_width="@dimen/hundred_dp"
                android:layout_height="wrap_content"
                android:text="Balance"/>
            <TextView
                android:textStyle="bold"
                android:id="@+id/textview10"
                android:textAlignment="center"
                android:textSize="@dimen/textSize"
                android:layout_margin="3dp"
                android:layout_width="@dimen/hundred_dp"
                android:layout_height="wrap_content"
                android:text="Location"/>

        </LinearLayout>


<!--    </android.support.v7.widget.CardView>-->
    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:foreground="@android:color/black"
        android:background="@android:color/black"/>
</LinearLayout>